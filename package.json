{
  "name": "rest-express",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx --watch server/index.ts",
    "build": "esbuild server/index.ts --platform=node --bundle --format=esm --target=node20 --outdir=dist --alias:@shared=./shared --alias:@=./client/src --alias:@assets=./attached_assets",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "twilio": "^5.10.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "esbuild": "^0.24.0",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
